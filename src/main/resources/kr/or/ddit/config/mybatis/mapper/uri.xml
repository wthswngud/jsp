<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="uri">
	<!-- 전체 목록 조회 -->
	<select id="selectAllList" resultType="uriVO">
		select	*
		from	urimapping
	</select>
	
	<!-- 일치하는 정보 조회 -->
	<select id="selectUri" parameterType="String" resultType="uriVO">
		SELECT	*
		FROM	URIMAPPING
		WHERE	URI = ${uri}
	</select>
</mapper>